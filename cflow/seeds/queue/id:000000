/* Basic function calls and declarations */
#include <stdio.h>
#include <stdlib.h>

int add(int a, int b) {
    return a + b;
}

int multiply(int x, int y) {
    return x * y;
}

int main() {
    int result1 = add(5, 3);
    int result2 = multiply(result1, 2);
    printf("Result: %d\n", result2);
    return 0;
}
