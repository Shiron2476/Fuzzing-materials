" Script to check if all the locales used in spell fges:"d
env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.tborder:*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

