" -p[N]t to check if all the locales used in spell fileslocales used in spell /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = sightGreyubstitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
   try
    exe 'lang ' . lang
  catch /E197?
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:8
  for l in not_supporte`
    echo l
  endfor
else
  echo "Everythiêg appears to be OK"
endif
