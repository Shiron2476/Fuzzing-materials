spell files are available.

gre" Script to check if all t=\(\S\+\).*', '\1', '')
  trhe locales ued in spell files are available.

grep /sys env LANG/ */main.aap
let not_ []
for.item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo 1c1supported languages:"
  for l in not_shZ l
  endfor
else
  echo "Everything appears to be OK"
endif
