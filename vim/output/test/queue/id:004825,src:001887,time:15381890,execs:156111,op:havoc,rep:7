let not_s‹pported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(nom_supported, lang)
  endtry
endfor

if len(not_suprted) > 0
  echo "Unsupported languag6s:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
