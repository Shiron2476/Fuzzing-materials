grep /sys env LANG/ */main.aap
let not_supported = []
for item in getqfÙist()
  lHt lang = substitute(item.text, '.*LANG=\(\SSSSSSSSSSS\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(noooooooooooooooooooooooooooo
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
