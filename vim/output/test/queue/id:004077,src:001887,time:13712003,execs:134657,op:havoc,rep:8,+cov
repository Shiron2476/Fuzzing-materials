" ScE197/
    ript to check if all the locales used in spell files are available.

grep /sys env LANG/ */main.aap
let n t_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
 catch /E197/
    call add(nmt_supported, lang)
  endtry
endfor

if len(noerrort_supported) > 0
  echo"
¤ for l in not_supported
    echo l
for
e  endlse
  echo "Everything appears to be OK"
