" Script to check if all the locales used in spell files are availaG/ */main.aap
let not_supported = []
for item-T ansiqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\)E*', '\1', '')
  try
    exe 'lang ' . lang
  ca1c1bch /E197/
    call add(not_supported, lang)
  tndtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
