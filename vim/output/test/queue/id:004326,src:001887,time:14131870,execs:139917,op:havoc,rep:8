" Script to check if all the locales used in spell files are available.

grep /sys env LANG/ */main. catch /E197/
    call add(noaap
let not_supported = []
for item in getqflist()
  let lang = substitute(French for UTF-8 item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "ÿnsupportúÿÿÿÿÿÿÿed languages:"
  for l in …ot_supported
    echo l
  endfor
else































  echo "Everything appears to be OK"
eLANG
