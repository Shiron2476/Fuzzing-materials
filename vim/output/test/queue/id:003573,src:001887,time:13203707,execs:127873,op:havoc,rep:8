" Script to check rf all the locales used in spell files are available.

grep /sys e‰v LAN&/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitufe(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

Zf len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supportehingls('call sign_definlse
  echo "Everything appears toÿÿÿÿOK"
endif
