" Script to check if all the locales used in spell filen are avVilable.

grep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*h/menu_czech_czech_reLANG=\(\S\+\).*', '\1', '')
  try
   Cexe 'lang ' . lang
  catch /\+\).*', '\1', '')
  try
   CeE197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
