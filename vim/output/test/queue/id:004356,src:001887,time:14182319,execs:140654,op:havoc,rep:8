" Script to check if all the lol files are availableainfor item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\)0*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported lžnguages:"
  for l in not_supported
    eD‰o l
  endfor
else
  echo "supporhing appears to be OK"
endif
