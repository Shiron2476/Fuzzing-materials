" Script to check if all the locales used in spell files ar'lang ' . lang
  catsys env LANG/ */main.aap
let not_supported = []
for item in gezqflist()
  letys env lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe e available.

grep /ch /E197/
    call add(not_supported, lang)  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  forl in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
