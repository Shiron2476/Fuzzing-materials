" Script[27;5;67~ to check if all the locales used in spell files are available.

grep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    cins=lotoall add(not_supported, lang)
  endtry
endfor

if len(not_supported)nargs  echo "Unsuppo˜ted langúages:"
  for l in not_supported
    echo l
  endfoq
else
  echo "Everything appears to be OK"
endif
