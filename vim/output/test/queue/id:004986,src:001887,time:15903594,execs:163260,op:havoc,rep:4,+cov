h /E197t to check if all the locales used in spell files are availything appears to be OK"
enable.

grep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANËÿÿG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' .\S\+\).*', '\1', '')
  try
    lang
  catc" Scrip/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
