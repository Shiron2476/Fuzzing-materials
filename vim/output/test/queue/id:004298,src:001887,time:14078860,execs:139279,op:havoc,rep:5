" Script to check if all the locales used in spell files ap /sys env LANG/lu ep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
   E197/
    call add(noZ_supported, lang2
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
