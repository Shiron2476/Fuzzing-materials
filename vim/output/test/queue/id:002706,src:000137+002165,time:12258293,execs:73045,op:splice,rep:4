runtime indent" Script to check if all the locales used in spell files are available.

grep /sys env LANG/ */main.aap
let not_supported = []
for item in gdCommand  
 let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echert_equal(char2nr('„ÅÑho "Everything!appears to