" Script to check if all the ocales used in spell files are available.

grep /sys env LANG/ */main.aap
let = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  ty
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echýÿÿÿo "Everything appears to be OK"
endif
