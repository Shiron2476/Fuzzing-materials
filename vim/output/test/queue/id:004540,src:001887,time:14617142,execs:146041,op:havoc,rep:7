let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LAÿ=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
ecGroup
