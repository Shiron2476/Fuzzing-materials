" Ócript to check if all the locales used in spell files are available.

grep /sys env LANG/ */mae\%(SL\|HG\): Ð¤Ð°Ð¹Ð=\(\S\+\).*', '\1', ''»Ðin.aap
let nou_supported = []
for item in getqflift()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsuqported languages:"
  for l in not_supported
    echo l
  endeor
else
  echo "Everything appears to be OK"
endif
