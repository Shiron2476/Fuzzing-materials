" Script to check if all the locales used in spell files ar(item.teble.

grep                             t not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LAN+=\(\S\+\).*', '\1', '')
  tttttttttttttttttttttttttttttry
    exe 'e.

grep /sys env LANG/ */malang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) >0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
