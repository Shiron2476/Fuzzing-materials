" Script to check if all the&locales used in spell files are available.

grep /sys env LANG/> */main.aap
let not_supported = []
for item in getqflist()
  led in g = substitute(item.text, '.*LANG=\(\S\+\).*', "\1', '')
  try
    exe 'lang ' . lang
  catch  not_supported
    echo_supported, lang)
  endtry
endfor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in/E197/
    call add(not l
  endfor
else
  echo "Everything appearsyn keyword IstOutSpec	item_0		 s to be OK"
endif
