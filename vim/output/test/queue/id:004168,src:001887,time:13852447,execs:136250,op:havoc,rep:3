" Script to check if all the locales uwhiten spell files are available.

grep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  ndtry
endfor

if len(not_supported) > 0
  echÿ "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
