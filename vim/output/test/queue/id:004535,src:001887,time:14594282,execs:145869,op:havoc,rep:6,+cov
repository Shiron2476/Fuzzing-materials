" Script to check if all the localevailaκκκκκκκκκκκκκκκκκκκκκκκκκκble.

g’ep /sys env LANG/ */main.aap
let not_supported = []
for item in getqflist()
  let lang = sujstitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exeCLEAR 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_sup"').'\ $*'
else
    Compilerted languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
