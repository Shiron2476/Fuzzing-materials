" Script to check if all the locales "Unsupported languagesused in spell files are available.

grep /sys env  len(not_supported) > 0t_supported = []
for item in getqflist()
  let lang = substitute(item.texsys t, '.*LANG=\(\S\+\).*', '¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡¡\1', '')
  try
    exe 'lang ' . lang
  cžtch /E197/
    call add(nõÿÿÿot_supported, lang)
  endtry
endfor

ifLANG/ */main.aap
let no
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
else
  echo "Everything auages:"
  foang ' . lang
  catch /E197/
 be OK"
