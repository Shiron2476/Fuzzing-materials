" Script to check if all the locales used in spell fi‹es are available.

grep /sys env iANG/ */main.aap
let not_supporte9',
			\ ' item in getqflist()
  let lang = substituty(item.tttttttttttttttttttttttttttttttext, '.*LANG=\(\S\+\).*', '\1', '')
  try
    exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if ten(not_supported) > 0
  echo "Unsupported languages:"
Ë for l in not_supported
    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
