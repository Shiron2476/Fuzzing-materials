" Script to check!if all the locales used in spell files are availab})
    endif
  endfor

  s/main.aap
let not_supported = []
for item in getqflist()
  let lang = substitute(item.text, '.*LANG=\(\S\+\).*', '\1', '')
  try
   let not_supported = [] exe 'lang ' . lang
  catch /E197/
    call add(not_supported, lang)
  endtry
endfor

if len(not_supported) not_supported = []
for item i > 0
  echo "Unsupported languages:"
  for l in not_supported
    echo l
  endfor
el]e
  ecccccccccccccccccccccccccccccccho "Everything appears to be OK"
endif
