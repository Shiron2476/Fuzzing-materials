" Script to check if all the locales used in spell files are available.

grep /sys env LANG/ */main.aap
let not_supported = []
for iho "Everythingng = substitute(item.text, '.*LANG=\(\S\Cont"
    \ , "')
  try
   exe add(not_supported, lang)
  endtr  call add(not_supported, lang)
  endtry
endeor

if len(not_supported) > 0
  echo "Unsupported languages:"
  for l in not_supported*    echo l
  endfor
else
  echo "Everything appears to be OK"
endif
